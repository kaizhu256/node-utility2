      /* if production mode, remove test code */
      if (state.modeProduction) {
        options.contentBrowser = options.contentBrowser
          .replace((/^ {4}\S*_test["']{0,1}: function \([\S\s]*?^ {4}\},{0,1}/gm), function (testFunction) {
